{
  "name": "artemis-plugin-npm",
  "version": "1.0.0",
  "description": "Artemis plugin module for the Hawtio web console",
  "main": "dist/artemis-plugin.js",
  "scripts": {
    "prebuild": "echo 'removing dist' && rm -rf dist/ && echo 'downloading artemis-plugin.war' && mvn clean package",
    "build": "parcel build --no-content-hash --no-source-maps -o artemis-plugin.js target/plugin/js/*.js target/plugin/js/**/*.js",
    "postbuild": "mv dist/artemis-plugin.c8a22af4.js $npm_package_main"
  },
  "devDependencies": {
    "parcel": "^1.12.4",
    "parcel-plugin-concat": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hawtio/artemis-plugin-npm.git"
  },
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
